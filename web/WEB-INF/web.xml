<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <welcome-file-list>
        <welcome-file>main.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>com.chekhovych.MainServlet1</servlet-name>
        <servlet-class>com.chekhovych.MainServlet1</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>com.chekhovych.MainServlet1</servlet-name>
        <url-pattern>/com.chekhovych.MainServlet1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>com.chekhovych.Validation</servlet-name>
        <servlet-class>com.chekhovych.Validation</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>com.chekhovych.Validation</servlet-name>
        <url-pattern>/com.chekhovych.Validation</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>com.chekhovych.Add_User</servlet-name>
        <servlet-class>com.chekhovych.Add_User</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>com.chekhovych.Add_User</servlet-name>
        <url-pattern>/com.chekhovych.Add_User</url-pattern>
    </servlet-mapping>
</web-app>